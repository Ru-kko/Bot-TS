  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ru-kko/Bot-TS/issues"
  },
  "homepage": "https://github.com/Ru-kko/Bot-TS#readme",
  "devDependencies": {
    "@types/node": "^18.11.8"
  }
}
