{
  "name": "bot",
  "version": "1.0.0",
  "private": true,
  "description": "This is a discord bot with a dashboard using stack \r - Express\r - Mysql\r - React\r - Nodejs",
  "scripts": {
    "build:lib": "pnpm run -parallel --filter \"./packages/**\" build",
    "build:lib-w": "pnpm run -parallel --filter \"./packages/**\" build-w",
    "build:serv": "pnpm run --filter \"./apps/server\" build",
    "build:client": "pnpm run --filter \"./apps/client\" build"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ru-kko/Bot-TS.git"
  },
  "keywords": [],
